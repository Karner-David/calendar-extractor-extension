{
  "manifest_version": 3,
  "name": "Event Extractor",
  "version": "1.0",
  "permissions": [
    "identity"
  ],

  "action": {
    "default_popup": "index.html"
  },

  "icons": {
    "16": "schedule16.png",
    "32": "schedule32.png",
    "128": "schedule128.png"
  },

  "oauth2": {
    "client_id": "677758899476-hpt6rc73gb1ugqqhnhrd1k9o6jks8hoc.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "host_permissions": [
    "https://calendar-extractor-extension.onrender.com/*"
  ]
}